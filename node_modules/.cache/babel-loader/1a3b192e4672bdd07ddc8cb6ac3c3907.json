{"ast":null,"code":"import Footer from '../components/Footer.vue';\nexport default {\n  name: 'RegisterItem',\n\n  data() {\n    return {\n      user: {\n        userId: '',\n        password: '',\n        userName: '',\n        userSex: 1\n      },\n      confirmPassword: ''\n    };\n  },\n\n  methods: {\n    checkUserId() {\n      this.$axios.post('UserController/getUserById', this.$qs.stringify({\n        userId: this.user.userId\n      })).then(response => {\n        if (response.data == 1) {\n          this.user.userId = '';\n          alert('此手机号码已存在！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n\n    register() {\n      if (this.user.userId == '') {\n        alert('手机号码不能为空！');\n        return;\n      }\n\n      if (this.user.password == '') {\n        alert('密码不能为空！');\n        return;\n      }\n\n      if (this.user.password != this.confirmPassword) {\n        alert('两次输入的密码不一致！');\n        return;\n      }\n\n      if (this.user.userName == '') {\n        alert('用户名不能为空！');\n        return;\n      } //注册请求\n\n\n      this.$axios.post('UserController/saveUser', this.$qs.stringify(this.user)).then(response => {\n        if (response.data > 0) {\n          alert('注册成功！');\n          this.$router.go(-1);\n        } else {\n          alert('注册失败！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n  },\n  components: {\n    Footer\n  }\n};","map":{"version":3,"mappings":"AA6DA;AACA;EACAA,oBADA;;EAEAC;IACA;MACAC;QACAC,UADA;QAEAC,YAFA;QAGAC,YAHA;QAIAC;MAJA,CADA;MAOAC;IAPA;EASA,CAZA;;EAaAC;IACAC;MACA;QACAN;MADA,IAEAO,IAFA,CAEAC;QACA;UACA;UACAC;QACA;MACA,CAPA,EAOAC,KAPA,CAOAC;QACAC;MACA,CATA;IAUA,CAZA;;IAaAC;MACA;QACAJ;QACA;MACA;;MACA;QACAA;QACA;MACA;;MACA;QACAA;QACA;MACA;;MACA;QACAA;QACA;MACA,CAhBA,CAiBA;;;MACA,+DACA,SADA,GAEAF,IAFA,CAEAC;QACA;UACAC;UACA;QACA,CAHA,MAGA;UACAA;QACA;MACA,CATA,EASAC,KATA,CASAC;QACAC;MACA,CAXA;IAYA;;EA3CA,CAbA;EA0DAE;IACAC;EADA;AA1DA","names":["name","data","user","userId","password","userName","userSex","confirmPassword","methods","checkUserId","then","response","alert","catch","error","console","register","components","Footer"],"sourceRoot":"src/views","sources":["Register.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <!-- header部分 -->\n    <header>\n      <p>用户注册</p>\n    </header>\n    <!-- 表单部分 -->\n    <ul class=\"form-box\">\n      <li>\n        <div class=\"title\">\n          手机号码：\n        </div>\n        <div class=\"content\">\n          <input type=\"text\" @blur=\"checkUserId\" v-model=\"user.userId\" placeholder=\"手\n机号码\">\n        </div>\n      </li>\n      <li>\n        <div class=\"title\">\n          密码：\n        </div>\n        <div class=\"content\">\n          <input type=\"password\" v-model=\"user.password\" placeholder=\"密码\">\n        </div>\n      </li>\n      <li>\n        <div class=\"title\">\n          确认密码：\n        </div>\n        <div class=\"content\">\n          <input type=\"password\" v-model=\"confirmPassword\" placeholder=\"确认密码\">\n        </div>\n      </li>\n      <li>\n        <div class=\"title\">\n          用户名称：\n        </div>\n        <div class=\"content\">\n          <input type=\"text\" v-model=\"user.userName\" placeholder=\"用户名称\">\n        </div>\n      </li>\n      <li>\n        <div class=\"title\">\n          性别：\n        </div>\n        <div class=\"content\" style=\"font-size: 3vw;\">\n          <input type=\"radio\" v-model=\"user.userSex\" value=\"1\"\n                 style=\"width:6vw;height: 3.2vw;\">男\n          <input type=\"radio\" v-model=\"user.userSex\" value=\"0\"\n                 style=\"width:6vw;height: 3.2vw;\">女\n        </div>\n      </li>\n    </ul>\n    <div class=\"button-login\">\n      <button @click=\"register\">注册</button>\n    </div>\n    <!-- 底部菜单部分 -->\n    <Footer></Footer>\n  </div>\n</template>\n<script>\nimport Footer from '../components/Footer.vue';\nexport default {\n  name: 'RegisterItem',\n  data() {\n    return {\n      user:{\n        userId:'',\n        password:'',\n        userName:'',\n        userSex:1\n      },\n      confirmPassword:''\n    }\n  },\n  methods: {\n    checkUserId(){\n      this.$axios.post('UserController/getUserById', this.$qs.stringify({\n        userId: this.user.userId,\n      })).then(response => {\n        if(response.data==1){\n          this.user.userId = '';\n          alert('此手机号码已存在！')\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    register() {\n      if (this.user.userId == '') {\n        alert('手机号码不能为空！');\n        return;\n      }\n      if (this.user.password == '') {\n        alert('密码不能为空！');\n        return;\n      }\n      if (this.user.password != this.confirmPassword) {\n        alert('两次输入的密码不一致！');\n        return;\n      }\n      if (this.user.userName == '') {\n        alert('用户名不能为空！');\n        return;\n      }\n      //注册请求\n      this.$axios.post('UserController/saveUser', this.$qs.stringify(\n          this.user\n      )).then(response => {\n        if(response.data>0){\n          alert('注册成功！');\n          this.$router.go(-1);\n        }else{\n          alert('注册失败！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  },\n  components: {\n    Footer\n  }\n}\n</script>\n<style scoped>\n/****************** 总容器 ******************/\n.wrapper {\n  width: 100%;\n  height: 100%;\n}\n/****************** header部分 ******************/\n.wrapper header {\n  width: 100%;\n  height: 12vw;\n  background-color: #0097FF;\n  color: #fff;\n  font-size: 4.8vw;\n  position: fixed;\n  left: 0;\n  top: 0;\n  z-index: 1000;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n/****************** 表单部分 ******************/\n.wrapper .form-box {\n  width: 100%;\n  margin-top: 12vw;\n}\n.wrapper .form-box li {\n  box-sizing: border-box;\n  padding: 4vw 3vw 0 3vw;\n  display: flex;\n  align-items: center;\n}\n.wrapper .form-box li .title {\n  flex: 0 0 18vw;\n  font-size: 3vw;\n  font-weight: 700;\n  color: #666;\n}\n.wrapper .form-box li .content {\n  flex: 1;\n}\n.wrapper .form-box li .content input {\n  border: none;\n  outline: none;\n  width: 100%;\n  height: 4vw;\n  font-size: 3vw;\n}\n.wrapper .button-login {\n  width: 100%;\n  box-sizing: border-box;\n  padding: 4vw 3vw 0 3vw;\n}\n.wrapper .button-login button {\n  width: 100%;\n  height: 10vw;\n  font-size: 3.8vw;\n  font-weight: 700;\n  color: #fff;\n  background-color: #38CA73;\n  border-radius: 4px;\n  border: none;\n  outline: none;\n}\n.wrapper .button-register {\n  width: 100%;\n  box-sizing: border-box;\n  padding: 4vw 3vw 0 3vw;\n}\n.wrapper .button-register button {\n  width: 100%;\n  height: 10vw;\n  font-size: 3.8vw;\n  font-weight: 700;\n  color: #666;\n  background-color: #EEE;\n  border-radius: 4px;\n  border: none;\n  outline: none;\n  border: solid 1px #DDD;\n}\n</style>"]},"metadata":{},"sourceType":"module"}